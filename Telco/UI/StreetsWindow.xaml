<Window x:Class="Telco.UI.StreetsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Telco.UI"
        mc:Ignorable="d"
        Title="Streets" Height="300" Width="400">
    <Grid DataContext="{Binding RelativeSource={RelativeSource Self}}">
        <!-- Элементы управления для отображения информации об улицах -->
        <DataGrid ItemsSource="{Binding Streets}" AutoGenerateColumns="True" Margin="10"/>
    </Grid>
</Window>
